<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    




    <!-- user deatails Table Start -->
    <div class="container-fluid pt-4 px-4">
        <div class="row g-4">
            
           
            <div class="col-12">
                <div class="bg-light rounded h-100 ">
                    {{!-- <h6 class="mb-4">Responsive Table</h6> --}}

                    <div id="wrapper" class="pt-5">

                        <!-- Content Wrapper -->
                        <div id="content-wrapper" class="d-flex flex-column pt-3">
                            <div class="container-fluid">
                                <!-- DataTales Example -->
                                <div class="card shadow mb-4">
                                    <div class="card-header py-3">
                                        <h6 class="m-0 font-weight-bold text-primary">Booked User Details</h6>
                                    </div>
                                    <div class="card-body">
                                        <div class="table-responsive">
                                            <table class="table table-bordered" id="dataTable" width="100%"
                                                cellspacing="0">
                                                <thead>
                                                    <tr>
                                                        <th>Name</th>
                                                        <th>Email</th>
                                                        <th>Phone Number</th>
                                                        <th>Date of Birth</th> 
                                                        <th>ID Card</th> 
                                                        <th>ID Card No</th> 
                                                        <th>Driving License No</th> 
                                                    </tr>
                                                </thead>
                                                
                                                <tbody>                                                    
                                                    <tr>
                                                        <td>{{bookedUserData.name}}</td>
                                                        <td>{{bookedUserData.email}}</td>
                                                        <td>{{bookedUserData.phonenumber}}</td>
                                                        <td>{{userDob}}</td>
                                                        <td>{{bookedUserData.idCard}}</td>
                                                        <td>{{bookedUserData.idCardNo}}</td>
                                                        <td>{{bookedUserData.drivingLicenceNo}}</td>                                                      
                                                    </tr>                                                  
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
          
        </div>
    </div>
    <!-- user deatails Table End -->




    <!-- booked car details Table Start -->
    <div class="container-fluid pt-4 px-4">
       
        <div class="row g-4 mt-3">
            
            <div class="col-12">
                
                <div class="bg-light rounded h-100 ">
                    
                    {{!-- <h6 class="mb-4">All Cars</h6> --}}

                    <div id="wrapper" class="pt-5">

                        <!-- Content Wrapper -->
                        <div id="content-wrapper" class="d-flex flex-column pt-3">
                            <div class="container-fluid">
                                <!-- DataTales Example -->
                                <div class="card shadow mb-4">
                                    <div class="card-header py-3">
                                        <h6 class="m-0 font-weight-bold text-primary">Booked Car Details
                                        </h6>
                                    </div>
                                    <div class="card-body">
                                        <div class="table-responsive">
                                            <table class="table table-bordered" id="dataTable" width="100%"
                                                cellspacing="0">                                              
                                                <thead>                                                   
                                                    <tr>
                                                        <th>Model Name</th>
                                                        <th>Transmission</th>
                                                        <th>Fuel</th>
                                                        <th>Seats</th>
                                                        <th>Plan</th> 
                                                        <th>Fare</th>
                                                        <th>Image</th>
                                                        <th>Deposit</th>
                                                        <th>Amount Paid</th>
                                                    </tr>
                                                </thead>
                                                <tbody >
                                                    <tr>
                                                        <td>{{bookedCarData.modelname}}</td>
                                                        <td>{{bookedCarData.transmission}}</td>
                                                        <td>{{bookedCarData.fueltype}}</td>
                                                        <td>{{bookedCarData.seats}}</td>
                                                        <td>{{returnedData.kmplan.kms}}</td>
                                                        <td>{{returnedData.kmplan.fare}}</td>
                                                        <td><img src="/images/Cars/{{bookedCarData.image}}" alt="" style="width: 45px;height: 45px;"></td>
                                                        <td>{{bookedCarData.deposit}}</td>
                                                        <td>{{returnedData.PaidAmount}}</td> 
                                                    </tr>
                                                </tbody> 
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
          
        </div>
    </div>
    <!--booked car details Table End -->




    <!-- Rental  details Table Start -->
    <div class="container-fluid pt-4 px-4">
       
        <div class="row g-4 mt-3">
            
            <div class="col-12">
                
                <div class="bg-light rounded h-100 ">
                    
                    {{!-- <h6 class="mb-4">All Cars</h6> --}}

                    <div id="wrapper" class="pt-5">

                        <!-- Content Wrapper -->
                        <div id="content-wrapper" class="d-flex flex-column pt-3">
                            <div class="container-fluid">
                                <!-- DataTales Example -->
                                <div class="card shadow mb-4">
                                    <div class="card-header py-3">
                                        <h6 class="m-0 font-weight-bold text-primary">Rental Details
                                        </h6>
                                    </div>
                                    <div class="card-body">
                                        <div class="table-responsive">
                                            <table class="table table-bordered" id="dataTable" width="100%"
                                                cellspacing="0">                                              
                                                <thead>                                                   
                                                    <tr>
                                                        <th>Model Name</th>
                                                        <th>Booked Date</th>
                                                        <th>Car Delivered</th>
                                                        <th>Car Returned</th> 
                                                        <th>Actual Kms Plan</th>
                                                        <th>Travelled Kms</th>
                                                       
                                                    </tr>
                                                </thead>
                                                <tbody >
                                                    <tr>
                                                        <td>{{updateExtrakm.modelname}}</td>
                                                        <td>{{bookedDate}}</td>
                                                        <td>{{deliveredDate}}</td>
                                                        <td>{{returnedDate}}</td>
                                                        <td>{{updateExtrakm.kmplan.kms}}</td>
                                                        <td>{{updateExtrakm.actualKmUsed}}</td>
                                                        
                                                    </tr>
                                                </tbody> 
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
          
        </div>
    </div>
    <!-- Rental  details Table End -->









    <!--======== Extra Fare details Table Start ==========-->
{{#if updateExtrakm.extraKmUsed}}
    <div class="container-fluid pt-4 px-4">
       
        <div class="row g-4 mt-3">
            
            <div class="col-12">
                
                <div class="bg-light rounded h-100 ">
                    
                    {{!-- <h6 class="mb-4">All Cars</h6> --}}

                    <div id="wrapper" class="pt-5">

                        <!-- Content Wrapper -->
                        <div id="content-wrapper" class="d-flex flex-column pt-3">
                            <div class="container-fluid">
                                <!-- DataTales Example -->
                                <div class="card shadow mb-4">
                                    <div class="card-header py-3">
                                        <h6 class="m-0 font-weight-bold text-primary">Extra Fare Details
                                        </h6>
                                    </div>
                                    <div class="card-body">
                                        <div class="table-responsive">
                                            <table class="table table-bordered" id="dataTable" width="100%"
                                                cellspacing="0">                                              
                                                <thead>                                                   
                                                    <tr>
                                                       
                                                        <th>Actual Kms</th> 
                                                        <th>Actual Amount Paid</th>                                                                                                               
                                                        <th>Extra Kms Used</th> 
                                                        <th>Extra Fare/kms</th>                                                                                                               
                                                        <th>Total Fare</th>                                                                                                               
                                                    </tr>
                                                </thead>
                                                <tbody >                                                   
                                                    <tr>
                                                        <td>{{updateExtrakm.kmplan.kms}}</td>
                                                        <td>{{updateExtrakm.PaidAmount}}</td>
                                                        <td>{{updateExtrakm.extraKmUsed}}</td>
                                                        <td>{{updateExtrakm.extrafare}}</td>
                                                        <td>{{updateExtrakm.extraFareCharged}}</td>
                                                                                                             
                                                    </tr>                                                  
                                                </tbody>                                               
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>          
        </div>
    </div>
    {{/if}}
     <!-- Extra Fare details Table End -->








    <!--======== Final Fare Earned  details Table Start ==========-->

    <div class="container-fluid pt-4 px-4">
       
        <div class="row g-4 mt-3">
            
            <div class="col-12">
                
                <div class="bg-light rounded h-100 ">
                    
                    {{!-- <h6 class="mb-4">All Cars</h6> --}}

                    <div id="wrapper" class="pt-5">

                        <!-- Content Wrapper -->
                        <div id="content-wrapper" class="d-flex flex-column pt-3">
                            <div class="container-fluid">
                                <!-- DataTales Example -->
                                <div class="card shadow mb-4">
                                    <div class="card-header py-3">
                                        <h6 class="m-0 font-weight-bold text-primary">Final Amount Recieved Details
                                        </h6>
                                    </div>
                                    <div class="card-body">
                                        <div class="table-responsive">
                                            <table class="table table-bordered" id="dataTable" width="100%"
                                                cellspacing="0">                                              
                                                <thead>                                                   
                                                    <tr>
                                                        <th>Amount Paid</th>
                                                        <th>Extra Fare</th>
                                                        <th>Total Amout</th>                                                                                                                                                                     
                                                    </tr>
                                                </thead>
                                                <tbody >                                                   
                                                    <tr>
                                                        <td>{{updateExtrakm.PaidAmount}}</td>

                                                    {{#if updateExtrakm.extraFareCharged}}
                                                        <td>{{updateExtrakm.extraFareCharged}}</td>
                                                    {{else}}
                                                        <td>0</td>
                                                    {{/if}}
                                                    {{#if updateExtrakm.finalAmount}}
                                                        <td>{{updateExtrakm.finalAmount}}</td>   
                                                    {{else}}   
                                                        <td>{{updateExtrakm.PaidAmount}}</td> 
                                                    {{/if}}                                                    
                                                    </tr>                                                   
                                                </tbody>                                               
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
          
        </div>
    </div>
     <!-- Final Fare Earned  details Table End -->














    <!-- Footer Start -->
    <div class="container-fluid pt-4 px-4">
        <div class="bg-light rounded-top p-4">
            <div class="row">
                <div class="col-12 col-sm-6 text-center text-sm-start">
                    &copy; <a href="#" style="color: #FC6414;">Alexa <span style="color: black;">Cars</span></a>, All Right Reserved.
                </div>
