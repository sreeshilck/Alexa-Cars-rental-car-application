<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    





<!-- payment deatails Table Start -->
    <div class="container-fluid pt-4 px-4">

        
        <div class="row g-4">
            <div class="col-12">
                <div class="bg-light rounded h-100 ">
       
                    {{!-- <h6 class="mb-4">Responsive Table</h6> --}}

                    <div id="wrapper" class="pt-5">

                        <!-- Content Wrapper -->
                        <div id="content-wrapper" class="d-flex flex-column pt-3">
                            <div class="container-fluid">
                                <!-- DataTales Example -->
                                <div class="card shadow mb-4">
                                    <div class="card-header py-3">
                                        <h6 class="m-0 font-weight-bold text-primary"> Cancelled Details</h6>
                                    </div>
                                    <div class="card-body">
                                        <div class="table-responsive">
                                            <table class="table table-bordered" id="dataTable" width="100%"
                                                cellspacing="0">
                                                <thead>
                                                    <tr>
                                                        <th>Booked Date</th>
                                                        <th>Cancelled Date</th>
                                                        <th>User Name</th>
                                                        <th>Delete</th>
                                                        
                                                    </tr>
                                                </thead>
                                               
                                                <tbody>                                                    
                                                    <tr>
                                                        <td>{{bookeddate}}</td>
                                                        <td>{{cancelleddate}}</td>
                                                        <td>{{CancelledData.bookedby}}</td>
                                                        <td class="text-center"><a href="/admin/deletecancel?id={{CancelledData._id}}"><i class="bi bi-trash" style="color: red;font-size: 24px;"></i></a></td>
                                                                                                            
                                                    </tr>                                                  
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
          
        </div>
    </div>
    <!-- payment deatails Table End -->









    


    <!-- booked car details Table Start -->
    <div class="container-fluid pt-4 px-4">
       
        <div class="row g-4 mt-3">
            
            <div class="col-12">
                
                <div class="bg-light rounded h-100 ">
                    
                    {{!-- <h6 class="mb-4">All Cars</h6> --}}

                    <div id="wrapper" class="pt-5">

                        <!-- Content Wrapper -->
                        <div id="content-wrapper" class="d-flex flex-column pt-3">
                            <div class="container-fluid">
                                <!-- DataTales Example -->
                                <div class="card shadow mb-4">
                                    <div class="card-header py-3">
                                        <h6 class="m-0 font-weight-bold text-primary">Cancelled Car Details
                                        </h6>
                                    </div>
                                    <div class="card-body">
                                        <div class="table-responsive">
                                            <table class="table table-bordered" id="dataTable" width="100%"
                                                cellspacing="0">                                              
                                                <thead>                                                   
                                                    <tr>
                                                        <th>Model Name</th>
                                                        <th>Transmission</th>
                                                        <th>Fuel</th>
                                                        <th>Seats</th>
                                                        <th>Plan</th> 
                                                        <th>Fare</th>
                                                        <th>Image</th>
                                                        <th>Deposit</th>
                                                     
                                                    </tr>
                                                </thead>
                                                <tbody >
                                                    <tr>
                                                        <td>{{CancelledCar.modelname}}</td>
                                                        <td>{{CancelledCar.transmission}}</td>
                                                        <td>{{CancelledCar.fueltype}}</td>
                                                        <td>{{CancelledCar.seats}}</td>
                                                        <td>{{CancelledData.kmplan.kms}}</td>
                                                        <td>{{CancelledData.kmplan.fare}}</td>
                                                        <td><img src="/images/Cars/{{CancelledCar.image}}" alt="" style="width: 45px;height: 45px;"></td>
                                                        <td>{{CancelledCar.deposit}}</td>
                                                      
                                                    </tr>
                                                </tbody> 
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
          
        </div>
    </div>
    <!--booked car details Table End -->














<!-- user deatails Table Start -->
    <div class="container-fluid pt-4 px-4">
        <div class="row g-4">
            <div class="col-12">
                <div class="bg-light rounded h-100 ">
                    {{!-- <h6 class="mb-4">Responsive Table</h6> --}}

                    <div id="wrapper" class="pt-5">

                        <!-- Content Wrapper -->
                        <div id="content-wrapper" class="d-flex flex-column pt-3">
                            <div class="container-fluid">
                                <!-- DataTales Example -->
                                <div class="card shadow mb-4">
                                    <div class="card-header py-3">
                                        <h6 class="m-0 font-weight-bold text-primary"> User Details</h6>
                                    </div>
                                    <div class="card-body">
                                        <div class="table-responsive">
                                            <table class="table table-bordered" id="dataTable" width="100%"
                                                cellspacing="0">
                                                <thead>
                                                    <tr>
                                                        <th>Name</th>
                                                        <th>Email</th>
                                                        <th>Phone Number</th>
                                                        <th>Date of Birth</th> 
                                                        <th>ID Card</th> 
                                                        <th>ID Card No</th> 
                                                        <th>Driving License No</th> 
                                                    </tr>
                                                </thead>
                                        
                                                <tbody>                                                    
                                                    <tr>
                                                        <td>{{CancelledUser.name}}</td>
                                                        <td>{{CancelledUser.email}}</td>
                                                        <td>{{CancelledUser.phonenumber}}</td>
                                                        <td>{{userDob}}</td>
                                                        <td>{{CancelledUser.idCard}}</td>
                                                        <td>{{CancelledUser.idCardNo}}</td>
                                                        <td>{{CancelledUser.drivingLicenceNo}}</td>                                                      
                                                    </tr>                                                  
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
          
        </div>
    </div>
    <!-- user deatails Table End -->










